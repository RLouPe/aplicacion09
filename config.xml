<?xml version="1.0" encoding="utf-8"?>
	<widget xmlns = "http://www.w3.org/ns/widgets"
		xmlns:gap = "http://phonegap.com/ns/1.0"
		id = "com.phonegap.example9"
		versionCode = "10"
		version = "1.0.0">
	
	<!-- versionCode is optional and Android only -->
	
	<name>Aplicacion 09 geolocalizador</name>
	
	<description>
	    app para geolocalizacion
	</description>
	
	<preference name="phonegap-version" value="3.7.0" />
	
	<preference name="target-device" value="universal" />
	<preference name="fullscreen" value="true" />
	<preference name="android-installLocation" value="auto" />
	
	<author href="https://build.phonegap.com" email="support@phonegap.com">
	    Jordan Vazquez Garcia
	</author>

		<gap:plugin name="org.apache.cordova.device" version="0.2.10" />
		<gap:plugin name="org.apache.cordova.vibration" version="0.3.0" />
		<gap:plugin name="org.apache.cordova.dialogs" version="0.2.6" />
		<plugin name="org.apache.cordova.geolocation" spec="0.3.12" source="pgb"/>
		

	<!-- Colocar icono app -->

<icon src="res/android/mdpi.png" />
</widget>